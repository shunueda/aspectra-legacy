{
  "name": "aspectra",
  "version": "0.0.1",
  "description": "Aspect-oriented programming!",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "format": "biome format --write",
    "lint": "biome lint --write",
    "check": "biome check --write",
    "commit": "git add --all && cz",
    "test": "tsx --test",
    "test:watch": "tsx --test --watch",
    "type-check": "tsc"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.1",
    "@types/node": "^22.5.5",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "lefthook": "^1.7.15",
    "tsx": "^4.19.1"
  },
  "peerDependencies": {
    "typescript": "^5.6.2"
  },
  "packageManager": "pnpm@9.10.0",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
